{
  "name": "@bluesh55/factory",
  "version": "1.4.3",
  "description": "Factory is the library for creating data from pre-configured attribute specs.",
  "main": "./dist/src/main.js",
  "scripts": {
    "test": "npx jest --runInBand"
  },
  "bin": {
    "factory": "./dist/bin/cli.js"
  },
  "keywords": [
    "test",
    "factory"
  ],
  "author": "bluesh55",
  "license": "MIT",
  "dependencies": {
    "@types/node": "^12.12.20",
    "typescript": "^3.7.3"
  },
  "devDependencies": {
    "@types/jest": "^25.1.2",
    "jest": "^24.9.0",
    "ts-jest": "^24.3.0"
  },
  "jest": {
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "testRegex": "\\.test\\.ts$",
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "globals": {
      "ts-jest": {
        "diagnostics": true
      }
    },
    "verbose": false
  }
}
